<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-50</chase-distance-m>

  <view n="100">
    <name>Copilote Seat View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.438 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -0.670 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -16.550 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg type="double">-15</pitch-offset-deg>
      <default-field-of-view-deg type="double">94</default-field-of-view-deg>
      <dynamic-view type="bool">true</dynamic-view>
    </config>
  </view>

  <view n="101">
    <name>Passenger Seat View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">  1.153 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y"> -0.602 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -5.694 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg type="double">0</pitch-offset-deg>
      <default-field-of-view-deg type="double">60</default-field-of-view-deg>
    </config>
  </view>

  <view n="102">
    <name>Passenger Seat2 View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">  -0.687 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -0.602 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -11.174 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg type="double">0</pitch-offset-deg>
      <default-field-of-view-deg type="double">60</default-field-of-view-deg>
      <heading-offset-deg type="double">180</heading-offset-deg>
    </config>
  </view>

  <view n="103">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  6.252 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 15.832 </z-offset-m> <!--Back-->
      <pitch-offset-deg type="double">-5</pitch-offset-deg>
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
    </config>
  </view>

  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  -0.438 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -0.670 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -16.550 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg type="double">-10</pitch-offset-deg>
      <default-field-of-view-deg type="double">94</default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

</PropertyList>
