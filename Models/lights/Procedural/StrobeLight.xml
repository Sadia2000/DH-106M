<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <animation>
    <type>select</type>
    <object-name>strobe</object-name>
    <nopreview/>
    <condition>
      <and>
        <property>/sim/model/lights/sbc1/state</property>
        <property>controls/lighting/strobe</property>
      </and>
    </condition>
  </animation>

  <light>
    <name>strobe</name>
    <type>point</type>
    <ambient>
      <r> 1 </r>
      <g> 1 </g>
      <b> 1 </b>
      <a> 1 </a>
    </ambient>
    <diffuse>
      <r> 1 </r>
      <g> 1 </g>
      <b> 1 </b>
      <a> 1 </a>
    </diffuse>
    <specular>
      <r> 1 </r>
      <g> 1 </g>
      <b> 1 </b>
      <a> 1 </a>
    </specular>
    <attenuation>
      <c> 1.00 </c>
      <l> 0.92 </l>
      <q> 2.40 </q>
    </attenuation>
    <range-m>30</range-m>
    <debug-color>
      <r> 1 </r>
      <g> 1 </g>
      <b> 1 </b>
      <a> 1 </a>
    </debug-color>
  </light>

  <animation>
    <type>noshadow</type>
    <object-name>strobe</object-name>
  </animation>

</PropertyList>
